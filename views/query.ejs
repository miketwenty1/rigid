<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Single page web app using Angularjs</title>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular-route.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
    <script src="script.js"></script>
</head>
<body>
<div>
    <div>
        <nav class="navbar navbar-inverse" role="navigation" style="padding-left:130px;">
            <ul class="nav navbar-nav">
                <li class="active"><a href="/trialee">Home<span class="sr-only">(current)</span></a></li>
                <li><a href="/about">About us</a></li>
                <li><a href="/contact">Contact us</a></li>
            </ul>
        </nav>
    </div>
    <br/>
    <div class="jumbotron">

        <p>

        <center><a href="/trialee">Query For Participant</a> </center>


        </p>

        <ul>
            <h2>All Participants</h2>
            <% datastore2.forEach(function(item) { %>
            <li>   <%= item.userids   %>
                <b><i> <%= item.consent %></i></b>
                <ul>
                    <li><b><i> MerkleRoot: </i></b> <%= item.sha256 %></li>

                    <li><b><i> Blockchain Receipt: </i></b> <%= item.receipt %></li>
                    <li>
                        <b><i> Proof: </i></b>
                        <a target="_blank" href='https://www.blocktrail.com/BTC/tx/<%= item.proof%>'>Tierion/Blocktrail cross ref source id: <%= item.proof%></a>
                    </li>
                </ul>
                <br>

                <% }); %>
            </li>
        </ul>
    </div>
</div>
</body>
</html>